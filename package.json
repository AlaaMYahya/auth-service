{
  "name": "madkhol_training",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "node index.js",
    "start:watch": "concurrently \"npx tsc --watch\" \"nodemon -q index.js\"",
    "ts-node": "ts-node",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "typeorm": "ts-node -r ./src/module-alias.ts ./node_modules/typeorm/cli",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "schema:sync": "typeorm schema:sync",
    "migration:show": "typeorm migration:show",
    "migration:generate": "typeorm migration:generate -n",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "migration:create": "typeorm migration:create -n",
    "migration:auto:generate": "typeorm migration:generate --name",
    "migrate:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts",
    "migrate:down": "ts-node --transpile-only ./node-modules/typeorm/cli.js migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/node": "^20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "prettier": "3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "migrations": "^1.3.0",
    "mysql2": "^3.6.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "description": ""
}
